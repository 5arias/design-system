{
  "extends": ["eslint:recommended", "plugin:react/recommended", "nava"],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/no-find-dom-node": 1
  }
}
